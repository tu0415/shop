page {
  height: 100%;
  background-color: #f1f1f1;
}
page .feedback {
  padding-top: 20rpx;
}
page .feedback .count {
  width: 100%;
  height: 400rpx;
}
page .feedback .count textarea {
  padding: 20rpx;
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
page .feedback .btn {
  width: 660rpx;
  border-radius: 40rpx;
  height: 80rpx;
}
page .feedback .btn::after {
  border: none;
}
